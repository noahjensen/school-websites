<header id="header" class="clearfix">
    <a href="index.php" class="logo">

         <span class="image"><%= image_tag "apptegy_cms/themes/littleriverwindomks/logo.png", alt: "Butteville UNION" %></span>
         <h1>USD #444<span>Little River-Windom</span></h1>
      </a>

                 <style> #header .logo, .cbp-spmenu-push #header .logo{z-index: 150;}</style>


                 <span class="search-icon"></span>

   <%= render 'apptegy_cms/shared/change_language_unstyled' %>

            <nav id="left-nav" class="nav" role="navigation">

            <div class="menu-btn"><span>Menu</span></div>

 <div class="menu-wrap">
          <ul class="static-links clearfix">

        <li class="live-feed">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/live_feed" : "/live_feed"%>>
                <span><%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/feed-icon.png' %></span>
               Live Feed
            </a>
        </li>
        <li class="news">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/news" : "/news"%>>
                <span><%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/news-icon.png' %> </span>
                News
            </a>
        </li>



            <li class="events">
                <a href=<%= params[:org].present? ? "/o/#{params[:org]}/events" : "/events"%>>
                  <span>  <%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/events.png' %> </span>
                    Events
                </a>
            </li>


<li class="dining">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/dining" : "/dining"%>>
                   <span> <%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/dining.png' %> </span>
                    Menu
                </a>
        </li>

 <li class="staff">
            <a href="/staff">
              <span>  <%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/staff.png' %> </span>
               Staff
            </a>
        </li>

        <li class="documents">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/documents" : "/documents"%>>
                <span> <%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/documents.png' %> </span>
                Documents
            </a>
        </li>

         <li class="athletics">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/athletics" : "/athletics"%>>
                <span> <%= image_tag 'apptegy_cms/themes/littleriverwindomks/menu/athletics.png' %> </span>
                Athletics
            </a>
        </li>


          </ul>
          <ul style="overflow:visible"  class="generated-links clearfix">
             <% current_pages.roots.navigation_pages.each do |page| %>
          <li>

              <% if page.header? %>
               <span> <%= page.name.upcase %> </span>
              <% else %>
                <a href=<%= page.page_link? ? "#{page.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{page.slug}" %> target=<%= '_blank' if page.page_link? %>>
                  <%= page.name %>
                </a>
              <% end %>

            <% if page.children.any? %>
              <ul>
                <% page.children.order(:position).each do |subpage| %>
                <% if subpage.children.any? %>
<li>

              <% if subpage.header? %>
               <span> <%= subpage.name %></span>
              <% else %>
                <a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                  <%= subpage.name %>
                </a>
              <% end %>

                  <ul>
                    <% subpage.children.order(:position).each do |subpage2| %>
                      <li>
                      <a href=<%= subpage2.page_link? ? "#{subpage2.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage2.slug}" %> target=<%= '_blank' if subpage2.page_link? %> >
                        <%= subpage2.name %>
                      </a>
                      </li>
                      <% end %>
                    </ul>
</li>
                  <% else %>
                  <li>
                    <a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                        <%= subpage.name %>
                    </a>
                  </li>
                  <% end %>
                <% end %>
              </ul>
            <% end %>
          </li>
        <% end %>
          </ul>
        <span class="menu-close"></span>
                </div>
            </nav>




    </header>

<%= render 'apptegy_cms/shared/search' %>
