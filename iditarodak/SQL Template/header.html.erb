<header id="header" class="clearfix">
<a class="logo" href="/">
         <%= image_tag "apptegy_cms/themes/iditarodak/logo.png", alt: "Havana Community Unit Schools" %>
         <h1>Iditarod Area School District <span><%= translate("Where the Future of Education is Happening Now") %></span></h1>
      </a>
      <span class="search-icon" ></span>
      <%= render 'apptegy_cms/shared/change_language' %>


            <nav id="left-nav" class="nav" role="navigation">

            <div class="menu-btn"><span>Menu</span></div>

 <div class="menu-wrap">
          <ul class="first-nav clearfix">

        <li class="live-feed">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/live_feed" : "/live_feed"%>>
                <span><%= image_tag 'apptegy_cms/themes/iditarodak/menu/feed-icon.png' %></span>
               Live Feed
            </a>
        </li>
        <li class="news">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/news" : "/news"%>>
                <span><%= image_tag 'apptegy_cms/themes/iditarodak/menu/news-icon.png' %> </span>
                News
            </a>
        </li>



            <li class="events">
                <a href=<%= params[:org].present? ? "/o/#{params[:org]}/events" : "/events"%>>
                  <span>  <%= image_tag 'apptegy_cms/themes/iditarodak/menu/events.png' %> </span>
                    Events
                </a>
            </li>

<li class="dining">
                    <a href=<%= params[:org].present? ? "/o/#{params[:org]}/dining" : "/dining"%>>
                   <span> <%= image_tag 'apptegy_cms/themes/iditarodak/menu/dining.png' %> </span>
                    Menu
                </a>
        </li>
 <li class="staff">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/staff" : "/staff"%>>
              <span>  <%= image_tag 'apptegy_cms/themes/iditarodak/menu/staff.png' %> </span>
               Staff
            </a>
        </li>
        <li class="documents">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/documents" : "/documents"%>>
                <span> <%= image_tag 'apptegy_cms/themes/iditarodak/menu/documents.png' %> </span>
                Documents
            </a>
        </li>
         <li class="athletics">
            <a href=<%= params[:org].present? ? "/o/#{params[:org]}/athletics" : "/athletics"%>>
                <span> <%= image_tag 'apptegy_cms/themes/iditarodak/menu/athletics.png' %> </span>
                Athletics
            </a>
        </li>

          </ul>
           <ul class="second-nav clearfix">
             <% current_pages.roots.navigation_pages.each do |page| %>
          <li class="item">

              <% if page.header? %>
               <span> <%= page.name.upcase %> </span>
              <% else %>
                <a href=<%= page.page_link? ? "#{page.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{page.slug}" %> target=<%= '_blank' if page.page_link? %>>
                  <%= page.name %>
                </a>
              <% end %>

            <% if page.children.any? %>
              <ul>
                <% page.children.order(:position).each do |subpage| %>
                <% if subpage.children.any? %>
<li>

              <% if subpage.header? %>
               <span> <%= subpage.name %></span>
              <% else %>
                <span><a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                  <%= subpage.name %>
                </a></span>
              <% end %>

                  <ul>
                    <% subpage.children.order(:position).each do |subpage2| %>
                      <li>
                      <a href=<%= subpage2.page_link? ? "#{subpage2.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage2.slug}" %> target=<%= '_blank' if subpage2.page_link? %> >
                        <%= subpage2.name %>
                      </a>
                      </li>
                      <% end %>
                    </ul>
</li>
                  <% else %>
                  <li>
                    <a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                        <%= subpage.name %>
                    </a>
                  </li>
                  <% end %>
                <% end %>
              </ul>
            <% end %>
          </li>
        <% end %>
          </ul>


        <span class="menu-close"></span>
                </div>
            </nav>


    </header>

<%= render 'apptegy_cms/shared/search' %>
