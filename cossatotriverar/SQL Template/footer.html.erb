<footer id="footer">
        <div class="container clearfix">
            <div class="footer-col">
                <h5><%= current_organization.try(:name)%></h5>
                <p><%= current_organization.try(:address) %>
                    <br><%="#{current_organization.try(:city)}, #{current_organization.try(:state)[-2..-1]} #{current_organization.try(:zip)}" %>
                    <br><%="PH: #{current_organization.try(:phone_number)}" %>
                    <br><%= "FAX #{current_organization.try(:fax_number)}" %></p>
            </div>
            <div class="footer-col schools-info">
              <h1> Schools </h1>
                <h5>Cossatot River High School &nbsp| </h5>
                <p>   PH (870) 385-7101 | FAX (870) 385-7101<br>

                    <h5>Wickes Elementary </h5>
                    <p>   PH (870) 385-2346 | FAX (870) 385-2242<br>

                        <h5>Vandervoort Elementary | </h5>
                        <p>   PH (870) 387-6923 | FAX (870) 387-6923 <br>

                          <h5>Umpire schools | </h5>
                          <p>   PH (870) 385-7101| FAX (870) 385-2238 </div>

            <div class="footer-col last">
                <% if current_organization[:facebook_link].present? %>
                <a href="<%= current_organization.try(:facebook_link) %>" class="fb">Facebook</a><a href="#" class="app-store">Download on the App Store</a>
                <% end %>
                <br> <% if current_organization[:twitter_link].present? %>
                <a href="<%= current_organization.try(:twitter_link) %>" class="tw">Twitter</a><a href="#" class="google-play">Get it on Google play</a> </div>
                <% end %>
        </div>
        <div class="container copyright">
            <p>Copyright &copy; Cossatot River School District 2017. All rights reserved. Powered by <a href="http://www.apptegy.com">apptegy</a></p>
        </div>
    </footer>
