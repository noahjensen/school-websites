        <body class="cbp-spmenu-push">

            <%=  page_header %>
                <!-- ======================== BREADCRUMB ======================== -->
                <div class="breadcrumb">
                    <ol>
                        <li>
                            <a href="/">
                                <%= current_pages.home_page.first.name %>
                            </a>
                        </li>
                        <li class="active">
                            <%= current_page.try(:name) %>
                        </li>
                    </ol>
                </div>

                <!-- ======================== MAIN CONTENT ======================== -->
                <%= cms_slider_menu %>
                    <div class="main" role="main">
                        <div class="container clearfix subpage">
                            <div class="right-content">
                                <%= yield :main_container %>
                            </div>
                        </div>
                    </div>
                    <!-- ======================== FOOTER ======================== -->
                    <%= page_footer %>
                        <!-- ======================== JAVASCRIPTS ======================== -->
 <script>
        $(document).ready(function() {

            if($('.group1').is(':visible')) {
                $(".group1").colorbox({
                    rel: 'group1'
                });
            }

        });

    </script>
        </body>

        </html>
