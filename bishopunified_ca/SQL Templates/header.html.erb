<header id="header" class="clearfix">
 <div class="container">
<% if params[:org] == "district" %>
<h1 class="logo"><a href=<%= "/o/#{params[:org]}" %>>
           <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-district.png", alt: "BISHOP UNIFIED SCHOOL district" %>  BISHOP UNIFIED SCHOOL DISTRICT <span>Ensuring Academic Excellence for All </span></a></h1>

           <% elsif params[:org] == "bes" %>
           <h1 class="logo"><a href=<%= "/o/#{params[:org]}" %>>
                      <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-elem.png", alt: "BISHOP UNION ELEMENTARY SCHOOL" %> BISHOP ELEMENTARY SCHOOL <span> RISING TO GREATNESS </span></a></h1>

                      <% elsif params[:org] == "buhs" %>
                      <h1 class="logo"><a href=<%= "/o/#{params[:org]}" %>>
                                 <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-bhs.png", alt: "BISHOP UNION HIGH SCHOOL" %>  BISHOP UNION HIGH SCHOOL <span>HOME OF THE BRONCOS </span></a></h1>

                                 <% elsif params[:org] == "pghs" %>
                                 <h1 class="logo"><a href=<%= "/o/#{params[:org]}" %>>
                                            <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-pal.png", alt: "PALISADE GLACIER HIGH SCHOOL" %>  PALISADE GLACIER HIGH SCHOOL <span>HOME OF THE PANTHERS </span></a></h1>

                                            <% elsif params[:org] == "hsms" %>
                                            <h1 class="logo"><a href=<%= "/o/#{params[:org]}" %>>
                                                       <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-hstreet.png", alt: "HOME STREET MIDDLE SCHOOL" %>  HOME STREET MIDDLE SCHOOL <span>HOME OF THE COLTS</span></a></h1>

<% else %>
<h1 class="logo"><a href="/">
           <%= image_tag "apptegy_cms/themes/bishopunifiedca/logo-district.png", alt: "BISHOP UNIFIED SCHOOL district" %> BISHOP UNIFIED SCHOOL district
<span>Quality <i class="dot"></i> Excellence <i class="dot"></i> Equity </span></a></h1>
<% end %>

           <span class="search-icon"></span>
            <nav id="right-nav" class="nav" role="navigation">
            <%= render 'apptegy_cms/shared/change_language_unstyled' %>
                <div class="menu-btn"><span><%= translate("Schools") %></span>
                    <div class="menu-wrap">
                    <ul>
                            <!--<%= organizations_menu %>-->
<% current_site.secondary_organizations.order(:position).each do |org| %>
<li>
<a href=<%= "/o/#{org.organization_alias.try(:parameterize)}" %>>
<%= org.name %>
</a>
</li>
<% end %>
                    </ul>
                   </div>
               </div>
            </nav>
            <nav id="left-nav" class="nav" role="navigation">
            <div class="menu-btn"><span class="menu_span">MENU</span>
            <div class="menu-wrap">
                        <ul class="first-nav clearfix">

                            <li class="live-feed">
                                <a href=<%=params[:org].present? ? "/o/#{params[:org]}/live_feed" : "/live_feed"%>>
                <span><%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/feed-icon.png' %></span>
                <div class="section">LIVE FEED</div>
            </a>
                            </li>
                            <li class="news">
                                <a href=<%=params[:org].present? ? "/o/#{params[:org]}/news" : "/news"%>>
             <span>   <%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/news-icon.png' %> </span>
                <div class="section">NEWS</div>
            </a>
                            </li>
                            <li class="events">
                                <a href=<%=params[:org].present? ? "/o/#{params[:org]}/events" : "/events"%>>
                <span>    <%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/events.png' %> </span>
                    <div class="section">EVENTS</div>
                </a>
                            </li>

                            <li class="dining">
                              <a href=<%=params[:org].present? ? "/o/#{params[:org]}/dining" : "/dining"%>>
                <span>    <%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/dining.png' %> </span>
                    <div class="section">Cafe Menu</div>
                </a>
                            </li>

                            <li class="staff">
                                <a href=<%=params[:org].present? ? "/o/#{params[:org]}/staff" : "/staff"%>>
                <span><%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/staff.png' %></span>
                <div class="section">STAFF</div>
            </a>
                            </li>

                            <li class="documents">
                                <a href=<%=params[:org].present? ? "/o/#{params[:org]}/documents" : "/documents"%>>
                <span><%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/documents.png' %> </span>
                <div class="section">DOCUMENTS</div>
            </a>
                            </li>


           <li class="athletics">
                                <a href=<%="/o/#{params[:org]}/athletics"%>>
                <span><%= image_tag 'apptegy_cms/themes/bishopunifiedca/menu/athletics.png' %></span>
                <div class="section">Athletics</div>
            </a>
                            </li>

                        </ul>

          <ul class="second-nav clearfix">
             <% current_pages.roots.navigation_pages.each do |page| %>
          <li>

              <% if page.header? %>
               <span> <%= page.name %> </span>
              <% else %>
                <a href=<%= page.page_link? ? "#{page.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{page.slug}" %> target=<%= '_blank' if page.page_link? %>>
                  <%= page.name %>
                </a>
              <% end %>

            <% if page.children.any? %>
              <ul>
                <% page.children.order(:position).each do |subpage| %>
                <% if subpage.children.any? %>
<li>

              <% if subpage.header? %>
               <span> <%= subpage.name %></span>
              <% else %>
                <a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                  <%= subpage.name %>
                </a>
              <% end %>

                  <ul>
                    <% subpage.children.order(:position).each do |subpage2| %>
                      <li>
                      <a href=<%= subpage2.page_link? ? "#{subpage2.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage2.slug}" %> target=<%= '_blank' if subpage2.page_link? %> >
                        <%= subpage2.name %>
                      </a>
                      </li>
                      <% end %>
                    </ul>
</li>
                  <% else %>
                  <li>
                    <a href=<%= subpage.page_link? ? "#{subpage.url}" : "#{ '/o/'.concat(params[:org]).concat('/page') if params[:org] }/#{subpage.slug}" %> target=<%= '_blank' if subpage.page_link? %> >
                        <%= subpage.name %>
                    </a>
                  </li>
                  <% end %>
                <% end %>
              </ul>
            <% end %>
          </li>
        <% end %>
          </ul>

                            <span class="menu-close"></span> </div>

               </div>
            </nav>
        </div>
        <%= render 'apptegy_cms/shared/search' %>
    </header>
